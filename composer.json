{
  "name": "digital-craftsman/cqrs",
  "description": "Reduced cost of change through CQRS in Symfony",
  "type": "symfony-bundle",
  "require": {
    "php": "8.2.*|8.3.*",
    "symfony/framework-bundle": "^5.4|^6.0",
    "symfony/serializer": "^5.4|^6.0"
  },
  "require-dev": {
    "digital-craftsman/ids": "0.9.*",
    "friendsofphp/php-cs-fixer": "^3.15",
    "phpunit/phpunit": "^9.5",
    "symfony/property-access": "^6.2",
    "symfony/property-info": "^6.2",
    "symfony/yaml": "^6.2",
    "vimeo/psalm": "^4.12"
  },
  "autoload": {
    "psr-4": {
      "DigitalCraftsman\\CQRS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DigitalCraftsman\\CQRS\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Kolb",
      "email": "info@digital-craftsman.de"
    }
  ]
}
