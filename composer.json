{
  "name": "digital-craftsman/cqrs",
  "description": "Reduced cost of change through CQRS in Symfony",
  "type": "symfony-bundle",
  "require": {
    "php": "8.2.*|8.3.*",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/serializer": "^6.4|^7.0"
  },
  "require-dev": {
    "digital-craftsman/ids": "0.13.*",
    "friendsofphp/php-cs-fixer": "^3.15",
    "phpunit/phpunit": "^10.5",
    "symfony/property-access": "^7.0",
    "symfony/property-info": "^7.0",
    "symfony/yaml": "^7.0",
    "vimeo/psalm": "^5.17"
  },
  "autoload": {
    "psr-4": {
      "DigitalCraftsman\\CQRS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DigitalCraftsman\\CQRS\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Kolb",
      "email": "info@digital-craftsman.de"
    }
  ]
}
